/* pages/trade/date-picker/date-picker.wxss */
.date-picker-layout{
  padding: 42rpx 20rpx;
}
.date-btn-item{
  height: 92rpx;
  line-height: 92rpx;
  background-color: #fff;
  font-size: 28rpx;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.date-btn-item .text{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 20rpx;
}
.date-btn-item .startTime{
  color: #999;
  position: absolute;
  right: 20rpx;
  top: 0;
}
.submit-btn{
  margin-top: 100rpx;
}