@import "/template/loading/loading";

page {
  background-color: #f7f7f7;
  /* padding-bottom: 100rpx; */
}

.container {
  overflow: hidden;
}

text {
  font-family: Microsoft YaHei;
  font-size: 26rpx;
  color: #666;
}

input {
  font-family: MicroSoft YaHei;
}

.ellipsis {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

/*底部小树林*/

.bcgd {
  width: 100%;
  height: 208rpx;
  position: fixed;
  bottom: -1rpx;
  left: 0;
  z-index: -1;
}

.empty {
  width: 200rpx;
  display: flex;
  flex-direction: column;
  margin: 306rpx auto 0;
}

.empty .empty-img {
  width: 104rpx;
  height: 118rpx;
  margin: 0 auto 20rpx;
}

.empty .empty-text {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #888;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.empty .empty-text text {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #888;
}

.loadOver {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999;
  margin: 30rpx auto 60rpx;
  text-align: center;
}

@keyframes btnGreenChange {
  0% {
    background: #54ac41;
  }

  100% {
    background: #348f21;
  }
}

@keyframes btnWhiteChange {
  0% {
    background: #fff;
  }

  100% {
    background: #eee;
  }
}

.btnGreenAnim {
  animation: btnGreenChange 0.1s;
}

.btnWhiteAnim {
  animation: btnWhiteChange 0.1s;
}

/*右侧箭头*/

.arrow {
  width: 16rpx;
  height: 16rpx;
  color: #c7c7cc;
  margin: auto 30rpx auto 20rpx;
  border: 3rpx #c7c7cc solid;
  border-top-color: transparent;
  border-left-color: transparent;
  transform: rotate(-45deg);
}

.back-home {
  position: fixed;
  right: 20rpx;
  bottom: 120rpx;
  width: 132rpx;
  height: 132rpx;
  border-radius: 50%;
  z-index: 999;
}

.lay-out {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 20;
}

.btn {
  width: 670rpx;
  height: 94rpx;
  margin: 0 auto;
  border-radius: 48rpx;
  text-align: center;
  line-height: 94rpx;
  color: #fff;
  font-weight: 500;
}

.btn-primiary {
  background: #19B329;
}

.noSroll {
  height: 100vh;
  overflow: hidden;
}

button::after {
  border: none;
}

.c-btn {
  margin: 0 auto;
  width: 670rpx;
  height: 94rpx;
  background: #19B329;
  border-radius: 47rpx;
  text-align: center;
}

.c-text {
  color: #fff;
  font-size: 36rpx;
  font-weight: 500;
  line-height: 97rpx;
}

.listBottom {
  margin-top: 20rpx;
  text-align: center;
  font-size: 24rpx;
  color: #999;
}

.submit-btn {
  width: 100%;
  height: 1005;
  background: #19B329;
  border-radius: 47rpx;
  color: #fff;
}

.list-hover {
  transform: scale(0.99);
  transition: all 0.1s;
}

.btn-hover {
  transition: all 0.01s;
  opacity: 0.8;
}

.flex-center {
  display: flex;
  justify-content: center;
  align-items: center;
}
.btn-round{
  width:380rpx;
  height:94rpx;
  background:linear-gradient(180deg,rgba(255,250,105,1) 0%,rgba(255,209,80,1) 100%);
  border-radius:48rpx;
  box-shadow: 0 5rpx 15rpx rgba(0,0,0,0.08);
  color:#E66C00;
  font-size:36rpx;
  font-weight: 600;
}
.button-hover{
  box-shadow:none;
}
.divider{
  margin-top:30rpx;
  background: #eee;
  height:2rpx;
}
/** 规则弹窗 **/
.tip-title{
  text-align: center;
  font-size:32rpx;
}
.tip-wrap{
  font-size:24rpx;
  color:#666;
  box-sizing: border-box;
  width:600rpx;
  padding:30rpx;
}
.tip-desc{
  padding:20rpx 0 0;
}
.tip-box{
  margin-top:20rpx;
}
.tip-box-content{
  line-height: 150%;
}
.tip-box-title{
  font-weight: 600;
}
.tip-important{
  margin-top:20rpx;
  color:#F56C6C;
  font-weight: 500;
}
/**
空数据
**/
.empty-box{
  padding-top:200rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.empty-image{
  width:244rpx;
  height:244rpx;
}
.empty-txt{
  font-weight: 500;
  margin-top:40rpx;
  color:#999;
  font-size:28rpx;
}
/**
弹窗关闭按钮
**/
.dialog--close {
  position: absolute;
  z-index: 9999;
  right:0;
  top:40rpx;
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  background-repeat: no-repeat;
  background-size: cover;
  background-image: url("data:image/svg+xml;base64,")
}