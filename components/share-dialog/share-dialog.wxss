.poster-dialog .no-vip {
  opacity: 0.9593;
  width: 610rpx;
  height: 758rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  color: rgba(255, 255, 255, 1);
  z-index: 111;
  text-align: center;
}

.no-vip-title {
  display: flex;
  justify-content: center;
  flex-direction: column;
  margin-top: 60rpx;
}

.no-vip-title text {
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  font-weight: 565;
  color: rgba(51, 51, 51, 1);
  line-height: 45rpx;
  text-align: center;
}

.no-card-no-spread {
  width: 386rpx;
  height: 386rpx;
  margin-top: 60rpx;
  opacity: 1;
}

.no-card {
  width: 236rpx !important;
  margin-right: 0rpx !important;
  color: #00b600 !important;
  border: 2rpx solid #00b600;
}

.no-card-buy {
  width: 313rpx !important;
  margin-right: 0rpx !important;
  border: 2rpx solid #00b600;
  background: #00b600;
  color: rgba(255, 255, 255, 1) !important;
}

.footer-btn-no-card-block {
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  position: absolute;
  bottom: 20rpx;
}

.footer-btn {
  display: block;
  width: 325rpx;
  height: 94rpx;
  border-radius: 47rpx;
  text-align: center;
  line-height: 94rpx;
}

.lay-out {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 20;
}
